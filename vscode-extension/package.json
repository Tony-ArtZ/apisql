{
  "name": "apisql-vscode",
  "icon": "./images/icon.png",
  "displayName": "ApiSQL",
  "description": "Language support for ApiSQL",
  "publisher": "TonyArtZ",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tony-ArtZ/apisql"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "apisql",
        "icon": {
          "light": "./images/icon.png",
          "dark": "./images/icon.png"
        },
        "aliases": [
          "ApiSQL",
          "apisql"
        ],
        "extensions": [
          ".apisql"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "apisql",
        "scopeName": "source.apisql",
        "path": "./syntaxes/apisql.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:apisql"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "build-local": "node build.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^16.17.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
